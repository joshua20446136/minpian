<!-- 幻灯 -->
<swiper indicator-dots="{{true}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" class="banner-box swiper-box ">
      <block wx:for="{{banner}}" wx:key="banner">
       
          <swiper-item>
            <image src="{{item.pic}}" data-src="{{item.link}}" class="slide-image"  bindtap="previewImage" />
          </swiper-item>
        
      </block>
</swiper>
<view class='container'>
  <!-- 4个按钮 -->
  <view class='buts4'>
      <view class='buts4o' bindtap="myc"><image class="src" src="../../image/pic/dao1.png"></image><view>家族荣耀</view></view>
      <view class='buts4o' bindtap="cmy"><image class="src" src="../../image/pic/dao2.png"></image><view>我的家族</view></view>
      <view class='buts4o' bindtap="mody"><image class="src" src="../../image/pic/dao3.png"></image><view>家族活动</view></view>
      <view class='buts4o' bindtap="share"><image class="src" src="../../image/pic/dao4.png"></image><view>家族动态</view></view>
  </view>


</view>